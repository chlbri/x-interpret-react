{
  "version": "0.1.6",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.mjs",
  "type": "module",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "config": "pnpm install",
    "rollup": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "build": "rm -rf lib && pnpm run rollup",
    "test": "vitest run --passWithNoTests",
    "test-watch": "pnpm test -- --watch --passWithNoTests",
    "remove": "rm -rf lib node_modules pnpm-lock.yaml",
    "clean": "pnpm run remove && pnpm run config",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "lint-watch": "onchange --await-write-finish 5500 \"**/*\" -- pnpm run lint",
    "postpublish": "git fetch & git pull",
    "prepublishOnly": "pnpm run build && pnpm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write \"**/**.{ts,tsx,json,js,jsx,yml}\""
    }
  },
  "name": "@bemedev/x-interpret-react",
  "author": "chlbri <bri_lvi@icloud.com>",
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-react": "^2.2.0",
    "esbuild": "^0.15.13",
    "eslint": "^8.27.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.2",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rollup": "^3.2.5",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "use-sync-external-store": "^1.0.0",
    "vite": "^3.2.3",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.25.1",
    "xstate": "^4.34.0"
  },
  "wallaby": {
    "slowTestThreshold": 200
  },
  "dependencies": {
    "@bemedev/decompose": "^0.1.6"
  }
}
